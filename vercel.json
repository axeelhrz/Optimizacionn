{
  "rewrites": [
    {
      "source": "/es/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/es/privacypolicy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/privacypolicy",
      "destination": "/privacy.html"
    },
    {
      "source": "/es",
      "destination": "/index.html"
    },
    {
      "source": "/es/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}