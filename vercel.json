{
  "rewrites": [
    {
      "source": "/es/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/es/privacypolicy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/privacypolicy",
      "destination": "/privacypolicy.html"
    },
    {
      "source": "/es",
      "destination": "/index.html"
    },
    {
      "source": "/es/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/terms.html",
      "destination": "/terms",
      "permanent": true
    },
    {
      "source": "/privacypolicy.html",
      "destination": "/privacypolicy",
      "permanent": true
    }
  ]
}